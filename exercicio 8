x1 = float(input("Me diga uma coordenada para um plano cartesiano P: "))
y1 = float(input("Me diga uma outra coordenada para este mesmo plano cartesiano P: "))
x2 = float(input("Me diga uma coordenada para um plano cartesiano Q: "))
y2 = float(input("Me diga uma outra coordenada para este mesmo plano cartesiano Q: "))
d = (((x2 - x1)**2) + ((y2 - y1)**2))**(1/2)
print("O plano cartesiano P tem as coordenadas {} e {}".format(x1, y1))
print("O plano cartesiano Q tem as coordenadas {} e {}".format(x2, y2))
print("A distância respectiva é: {:.3f}".format(d))
